{
  "name": "postbook",
  "version": "0.2.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "echo 'Teste'"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "re-carousel": "^2.4.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "yarn": "^1.19.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-a11y/alt-text": [
        2,
        {
          "elements": [
            "img",
            "object",
            "area",
            "input[type=\"image\"]"
          ],
          "img": [
            "Image"
          ],
          "object": [
            "Object"
          ],
          "area": [
            "Area"
          ],
          "input[type=\"image\"]": [
            "InputImage"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^3.0.9"
  }
}
